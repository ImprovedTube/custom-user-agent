{
	"manifest_version": 2,
	"name": "User-Agent",
        "description": "User-Agent Switcher like never before! User-Agent by domain url regex wildcard. Keep Mobile version. Top100 User-Agent UA UserAgent",
	"version": "1.3",
	"default_locale": "en",
	"icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_popup": "index.html"
	},
	"options_page": "options.html",
	"permissions": [
		"storage",
		"<all_urls>",
		"webNavigation",
		"webRequest",
		"webRequestBlocking"
	]
}
